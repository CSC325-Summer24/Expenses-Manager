<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.view.ReportFormController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Report Form">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="titleField" layoutX="14.0" layoutY="42.0" prefWidth="570.0" promptText="Title" />
        <TextArea fx:id="contentField" layoutX="14.0" layoutY="82.0" prefHeight="200.0" prefWidth="570.0" promptText="Content" />
        <DatePicker fx:id="dateField" layoutX="14.0" layoutY="292.0" prefWidth="570.0" />
        <Button fx:id="saveButton" layoutX="250.0" layoutY="332.0" mnemonicParsing="false" text="Save" onAction="#handleSaveButtonAction" />
    </children>
</AnchorPane>
